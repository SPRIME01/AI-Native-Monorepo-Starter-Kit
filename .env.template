# .env.template - AI/ML Environment Variables
# Copy to .env and fill in your secrets and config for local/dev/prod

# ──────────────── LLM Providers ────────────────
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
COHERE_API_KEY=

# ──────────────── Vector DBs ────────────────
PINECONE_API_KEY=
CHROMADB_URL=
WEAVIATE_URL=

# ──────────────── Model Config ────────────────
MODEL_PROVIDER=openai
MODEL_NAME=gpt-4
MODEL_ENV=dev  # dev, staging, prod

# ──────────────── Observability ────────────────
LANGSMITH_API_KEY=
DEEPEVAL_API_KEY=

# ──────────────── Deployment ────────────────
RAY_SERVE_URL=
SAGEMAKER_ENDPOINT=

# ──────────────── Other ────────────────
SECRET_KEY=

# ──────────────── Supabase ────────────────
SUPABASE_URL=http://localhost:54323
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-key
